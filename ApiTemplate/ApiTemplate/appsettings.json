{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=127.0.0.1,1433;Database=test2;User Id=sa;Password=YourStrongPassword!;TrustServerCertificate=True;"
  },
  "JWTSetting": {
    "ValidAudience": "User",
    "ValidIssuer": "Api",
    "securitykey": "this is a secret key where i put my key structure"
  },
  "Authentication": {
    "EnableWindowsAuthentication": false
  },
  "Cors": {
    "AllowedOrigins": [
      "https://localhost:7239",
      "http://127.0.0.1:5501",
      "http://localhost:5501"
    ]
  },
  "Version": {
    "DefaultMajor": 1,
    "DefaultMinor": 0,
    "Current": "1.0"
  },
  "EmailSettings": {
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 587,
    "EnableSsl": true,
    "Username": "",
    "Password": "",
    "FromEmail": "",
    "FromName": "",
    "TimeoutSeconds": 30
  },
  "Serilog": {
    "FilePath": "Logs/log-.json",
    "TextFilePath": "Logs/log-.txt",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "RateLimiting": {
    "FixedPolicy": {
      "PermitLimit": 10,
      "WindowMinutes": 1,
      "QueueLimit": 2
    },
    "SlidingPolicy": {
      "PermitLimit": 100,
      "WindowMinutes": 1,
      "SegmentsPerWindow": 4,
      "QueueLimit": 5
    },
    "TokenPolicy": {
      "TokenLimit": 100,
      "QueueLimit": 5,
      "ReplenishmentPeriodSeconds": 10,
      "TokensPerPeriod": 20,
      "AutoReplenishment": true
    },
    "ConcurrencyPolicy": {
      "PermitLimit": 10,
      "QueueLimit": 5
    },
    "PerIPPolicy": {
      "PermitLimit": 100,
      "WindowMinutes": 1,
      "AutoReplenishment": true
    },
    "PerUserPolicy": {
      "PermitLimit": 50,
      "WindowMinutes": 1,
      "SegmentsPerWindow": 4,
      "AutoReplenishment": true
    },
    "TieredPolicy": {
      "Premium": {
        "TokenLimit": 1000,
        "ReplenishmentPeriodMinutes": 1,
        "TokensPerPeriod": 1000
      },
      "Basic": {
        "TokenLimit": 100,
        "ReplenishmentPeriodMinutes": 1,
        "TokensPerPeriod": 100
      },
      "Anonymous": {
        "TokenLimit": 10,
        "ReplenishmentPeriodMinutes": 1,
        "TokensPerPeriod": 10
      }
    },
    "GlobalLimiter": {
      "PermitLimit": 10,
      "WindowMinutes": 1,
      "AutoReplenishment": true
    },
    "OnRejected": {
      "StatusCode": 429,
      "Message": "Too many requests. Please try again later.",
      "RetryAfterMessage": "Too many requests. Please try again after {0} minute(s)."
    }
  }
}